        "formsnap": "^0.5.0",
        "lucide-svelte": "^0.335.0",
        "mode-watcher": "^0.2.1",
        "oslo": "^1.1.2",
        "svelte-headless-table": "^0.18.2",
        "sveltekit-superforms": "^2.5.0",
        "tailwind-merge": "^2.2.1",
        "tailwind-variants": "^0.2.0",
        "ts-todo": "git+https://github.com/Odilf/ts-todo.git#4f5d78e",
        "zod": "^3.22.4"
